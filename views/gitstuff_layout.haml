%html
  %head
    %title gitstuff
    
    :plain
      <script type="text/javascript" src="http://use.typekit.com/omo5tya.js"></script>
      <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
    
    :css
      * {
        margin: 0; padding: 0
      }

      body {
        background: #fbfaf6;
        font-family: chaparral-pro, Georgia, serif;
        color: #666;
      }
      
      p {
        font-size: 1.2em;
        margin-bottom: 0.8em;
      }

      #main {
        width: 500px;
        margin: 0 auto;
        text-align: center;
        margin-top: 50px;
      }
      
      h1, h2 {
        margin: 0 auto 10px auto;
        font-weight: normal;
      }
      
      h1 {
        padding: 20px;
        font-size: 3.2em;
        text-shadow: 1px 1px 1px #ccc;
      }
      
      h2 {
        font-size: 1.2em;
      }

      a {
        color: #666;
      }
      
      .credits a {
        text-decoration: none;
      }

      a:hover {
        color: #000;
      }
      
      .credits {
        margin-top: 40px;
      }
      
      .symbol {
        font-family: Helvetica, Arial, sans-serif;
      }
  %body
    #main
      = yield
  